  "nodes": [
    {
      "type": "hypothesis",
      "text": "Antioxidants are compounds capable of accepting electrons, such as O2 or F2.",
      "strength": 0.10291511400983727,
      "id": "H0"
    },
    {
      "type": "hypothesis",
      "text": "Oxygen is a compound capable of accepting electrons, such as O2 or F2.",
      "strength": 0.9694924806919091,
      "id": "H1"
    },
    {
      "type": "hypothesis",
      "text": "Residues are compounds capable of accepting electrons, such as o2 or f2.",
      "strength": 0.9482797908184777,
      "id": "H2"
    },
    {
      "type": "hypothesis",
      "text": "Oxidants are compounds capable of accepting electrons, such as O2 or F2.",
      "strength": 0.9694924806919091,
      "id": "H3"
    },
    {
      "type": "argument",
      "text": "An oxidizing agent (also known as an oxidant, oxidizer, electron recipient, or electron acceptor) is a substance in a redox chemical reaction that gains or 'accepts'/'receives' an electron from a reducing agent (called the reductant, reducer, or electron donor).",
      "strength": 0.9798681044077121,
      "id": "A4"
    },
    {
      "type": "argument",
      "text": "In other words, an oxidizer is any substance that oxidizes another substance.",
      "strength": 0.9798649857340795,
      "id": "A5"
    },
    {
      "type": "argument",
      "text": "Electron acceptors participate in electron-transfer reactions.",
      "strength": 0.9803007688855467,
      "id": "A6"
    },
    {
      "type": "argument",
      "text": "In this context, the oxidizing agent is called an electron acceptor and the reducing agent is called an electron donor.",
      "strength": 0.985598381344649,
      "id": "A7"
    }
  ],
